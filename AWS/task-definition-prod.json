{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "family": "LearningJenkinsApp-TaskDefinition-Preprod",
    "containerDefinitions": [
        {
            "name": "learnjenkinsapp",
            "image": "886436949623.dkr.ecr.us-east-1.amazonaws.com/myjenkinsapp:1.0.150",
            "portMappings": [{
                "name":"nginx-80-tip",
                "containerPort":80,
                "hostPort": 80,
                "protocol":"tcp",
                "appProtocol": "http"
            }],
            "essential": true
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::886436949623:role/ecsTaskExecutionRole"
}
